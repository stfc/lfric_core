#!jinja2
{%- set projectName = 'skeleton' %}
{%- set projectDir  = 'skeleton' %}
{%- set miniappFlag = True %}
{%- set fix_enums = False %}
{%- set appDiagStemName = 'diagSkeleton' %}
{%- set rdef_precision = "64" %}
{%- set mesh_rdef_precision = "64" %}

{%- set application_configurations = { 'skeleton' : ['default'] } %}

{%- set canned_configurations  = ['skeleton'] %}

{%- set groups = {
'developer': [
    'run_application("skeleton", "default", resolutions=[("C24",)])',
    'check_compilers("skeleton", "default", "fast-debug", resolution=("C24",))',
    'check_compilers("skeleton", "default", "full-debug", resolution=("C24",))',
    'canned_test("skeleton")',
    'godliness',
    'cleanliness'],
'nightly': [
    'cleanliness'] } %}

#########################################################################################

{# Include any local miniapp include files here #}
%include inc/MyMiniappIncludeFile.rc
